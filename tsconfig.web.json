{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/env.d.ts",
    "src/renderer/**/*",
    "src/renderer/**/*.tsx",
    "src/preload/*.d.ts",
    "src/typings/**/*"
 ],
  "compilerOptions": {
    "composite": true,
    "noImplicitAny": true,
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
  }
}
